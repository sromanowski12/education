{
  "version": "0.2.0",
  "configurations": [
    
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/voice-assistant-net-server-main/VoiceAssistant.Server/bin/Debug/net5.0/VoiceAssistant.Server.dll",
      "args": [],
      "cwd": "${workspaceFolder}/voice-assistant-net-server-main/VoiceAssistant.Server",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Users/sromanowski/source/repos/education",
      "program": "c:/Users/sromanowski/source/repos/education/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}